// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Dust {
  id String @id
  sido String
  station String
  value Decimal
  grade Int
  bookmarked Boolean @default(false)
}